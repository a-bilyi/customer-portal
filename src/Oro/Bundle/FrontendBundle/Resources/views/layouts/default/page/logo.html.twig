{% block _logo_widget %}
    {% set imgUrl = asset('bundles/orofrontend/blank/images/logo/logo-oroacem.svg') %}

    {% set attr_img = attr_img|default({}) %}
    {% set attr_img = layout_attr_defaults(attr_img, {
        'src': imgUrl
    }) %}

    {{ parent_block_widget(block) }}
{% endblock %}

{% block _logo_print_widget %}
    {{ block('_logo_widget') }}
{% endblock %}
